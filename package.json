{
  "name": "Instagram",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "env": "npx @siliconjungles/rn-scripts replace_files path=env/${ENV}/${ENV}.env.cfg",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "ENV=${ENV:-local} yarn env && react-native start",
    "test": "jest",
    "test_coverage": "yarn test -- --coverage --collectCoverageFrom=**/src/utils/**",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint_fix": "yarn lint --fix --plugin unused-imports --rule 'unused-imports/no-unused-imports-ts: warn'",
    "compile": "tsc --noEmit && yarn lint",
    "storybook": "ENV=storybook yarn start",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-dropdownalert": "^4.3.0",
    "react-native-modal": "^11.5.6",
    "reactjs-commons": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@siliconjungles/rn-scripts": "^0.0.0-41",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react-native": "^5.3.21",
    "@storybook/react-native-server": "^5.3.21",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-unused-imports": "^0.1.3",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "16.13.1",
    "typescript": "3.9.7"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
